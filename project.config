	forgeAuthor = group v8-tag-committers
	forgeCommitter = group v8-tag-committers
	push = group v8-tag-committers
	pushMerge = group v8-tag-committers
[access "refs/tags/lkgr/*"]
	exclusiveGroupPermissions = create forgeAuthor forgeCommitter push
	create = group v8-autoroll
	forgeAuthor = group v8-autoroll
	forgeCommitter = group v8-autoroll
	push = group v8-autoroll
[access "refs/heads/master"]
	exclusiveGroupPermissions = push submit
	push = group v8-autoroll
	push = group v8-committers
[access "refs/heads/tbd"]
	exclusiveGroupPermissions = push
	push = group v8-autoroll
	push = group v8-committers
[mimetype "text/x-torque"]
        path = *.tq
[plugin "git-numberer"]
	generate-enabled-refglob = refs/heads/master
	generate-enabled-refglob = refs/heads/playground
	generate-enabled-refglob = refs/heads/*.*.*
	generate-enabled-refglob = refs/branch-heads/*.*
	generate-enabled-refglob = refs/branch-heads/test-*
	generate-enabled-refglob = refs/internal/*
	generate-enabled-refglob = refs/heads/main
	validate-enabled-refglob = refs/heads/master
	validate-enabled-refglob = refs/heads/playground
	validate-enabled-refglob = refs/heads/*.*.*
	validate-enabled-refglob = refs/branch-heads/*.*
	validate-enabled-refglob = refs/branch-heads/test-*
	validate-enabled-refglob = refs/internal/*
	validate-enabled-refglob = refs/heads/main
[plugin "zosscoverage"]
	enable = false
	enableLowCoverageAlerting = false
	enableVoting = false
	incrementalCoverageDisplayErrorBelow = 0
	incrementalCoverageDisplayWarningBelow = 0
	labelName = Code-Coverage
	minAffectedFilesToEnforce = 0
	minAffectedLinesToEnforce = 0
	negativeVoteValue = -1
	positiveVoteValue = 1
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
[submit-requirement "Code-Review"]
	description = Submit requirement for the 'Code-Review' label
	submittableIf = label:Code-Review=MAX,user=non_uploader -label:Code-Review=MIN
	overrideIf = label:Bot-Commit=+1 AND -label:Code-Review=MIN
	canOverrideInChildProjects = true
[submit-requirement "Verified"]
	description = Submit requirement for the 'Verified' label
	applicableIf = is:false
	submittableIf = is:true
	canOverrideInChildProjects = true
