[access]
	inheritFrom = v8
[submit]
	action = rebase always
	mergeContent = true

[label "Code-Review"]
	value = -1 Don't submit as-is
	value = 0 No score
	value = +1 Looks good to me
	defaultValue = 0
	function = NoBlock
	ignoreSelfApproval = true
	copyCondition = changekind:NO_CHANGE OR changekind:NO_CODE_CHANGE OR changekind:TRIVIAL_REBASE OR has:unchanged-files OR is:MIN
[label "Commit-Queue"]
	value = 0 Not ready
	value = +1 Dry run
	value = +2 Commit
	defaultValue = 0
	function = NoBlock
	copyCondition = changekind:NO_CHANGE
[label "Verified"]
	function = NoBlock
	defaultValue = 0
	copyCondition = changekind:NO_CHANGE

[access "refs/for/*"]
	push = group Logged-in Public Users
	create = group Logged-in Public Users
	forgeAuthor = group v8-committers
	addPatchSet = group v8-committers
	addPatchSet = group v8-scoped@luci-project-accounts.iam.gserviceaccount.com
[access "refs/heads/*"]
	label-Code-Review = -1..+1 group v8-committers
	submit = group v8-committers
	label-Auto-Submit = +0..+1 group Change Owner
	label-Commit-Queue = +0..+2 group Change Owner
	label-Commit-Queue = +0..+2 group v8-committers
	labelAs-Commit-Queue = +0..+2 group v8-admins
	labelAs-Commit-Queue = +0..+2 group v8-scoped@luci-project-accounts.iam.gserviceaccount.com
	submit = group v8-scoped@luci-project-accounts.iam.gserviceaccount.com
	push = +force group v8-autoroll
	push = group v8-committers
	forgeAuthor = group v8-autoroll
	create = group chromium-branch-creators
	create = group v8-autoroll
	forgeAuthor = group v8-committers
	forgeCommitter = group v8-autoroll
	forgeCommitter = group v8-committers
	abandon = group v8-committers
	label-Owners-Override = +0..+1 group cria/project-v8-sheriffs

[access "refs/tags/*"]
	create = group v8-autoroll
	create = group v8-ci-pgo-builder
	create = group v8-committers
	push = group v8-autoroll
	push = group v8-ci-pgo-builder
	push = group v8-committers
	forgeAuthor = group v8-autoroll
	forgeAuthor = group v8-ci-pgo-builder
	forgeAuthor = group v8-committers
	forgeCommitter = group v8-autoroll
	forgeCommitter = group v8-ci-pgo-builder
	forgeCommitter = group v8-committers
[access "refs/branch-heads/*"]
	create = group chromium-branch-creators
	create = group v8-autoroll
	forgeAuthor = group v8-autoroll
	forgeAuthor = group v8-committers
	forgeCommitter = group v8-autoroll
	forgeCommitter = group v8-committers
	push = group v8-autoroll
	push = group v8-committers
	label-Code-Review = -1..+1 group v8-committers
	label-Commit-Queue = +0..+2 group Change Owner
	label-Commit-Queue = +0..+2 group v8-committers
	labelAs-Commit-Queue = +0..+2 group v8-scoped@luci-project-accounts.iam.gserviceaccount.com
	submit = group v8-committers
	submit = group v8-scoped@luci-project-accounts.iam.gserviceaccount.com
	label-Owners-Override = +0..+1 group cria/project-v8-sheriffs
	abandon = group v8-committers

[access "refs/heads/candidate"]
